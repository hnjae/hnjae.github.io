&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> css &lt;span style="color:#f92672">=&lt;/span> css_min&lt;span style="color:#f92672">.&lt;/span>with_suffix(&lt;span style="color:#e6db74">&amp;#39;&amp;#39;&lt;/span>)&lt;span style="color:#f92672">.&lt;/span>with_suffix(&lt;span style="color:#e6db74">&amp;#39;.css&amp;#39;&lt;/span>)
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> cssRatios&lt;span style="color:#f92672">.&lt;/span>append([css&lt;span style="color:#f92672">.&lt;/span>name,
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> &lt;span style="color:#e6db74">&amp;#34;&lt;/span>&lt;span style="color:#e6db74">{:.0f}&lt;/span>&lt;span style="color:#e6db74">% minified (&lt;/span>&lt;span style="color:#e6db74">{:4.1f}&lt;/span>&lt;span style="color:#e6db74"> KiB)&amp;#34;&lt;/span>&lt;span style="color:#f92672">.&lt;/span>format( &lt;span style="color:#ae81ff">100&lt;/span> &lt;span style="color:#f92672">*&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> css_min&lt;span style="color:#f92672">.&lt;/span>stat()&lt;span style="color:#f92672">.&lt;/span>st_size &lt;span style="color:#f92672">/&lt;/span> css&lt;span style="color:#f92672">.&lt;/span>stat()&lt;span style="color:#f92672">.&lt;/span>st_size,
